{
  "mcpServers": {
    "sequentialthinking": {
      "timeout": 60,
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-sequential-thinking"
      ]
    },
    "github": {
      "timeout": 60,
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-github"
      ]
    },
    "brave-search": {
      "command": "npx",
      "args": [
        "envmcp",
        "-e",
        "~/.devcontainer/.env.devcontainer",
        "npx",
        "-y",
        "@modelcontextprotocol/server-brave-search"
      ]
    },
    "context7": {
      "timeout": 60,
      "command": "npx",
      "args": [
        "-y",
        "@upstash/context7-mcp@latest"
      ]
    }
  }
}
